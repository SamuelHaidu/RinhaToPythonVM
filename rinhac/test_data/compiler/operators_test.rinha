// Operators
let add_fn = fn(a, b) => {
    a + b
};

// Subtração:
let sub_fn = fn(a, b) => {
    a - b
};

// Multiplicação:
let mul_fn = fn(a, b) => {
    a * b
};

// Divisão:
let div_fn = fn(a, b) => {
    a / b
};

// Resto da divisão:
let rem_fn = fn(a, b) => {
    a % b
};

// Igualdade:
let eq_fn = fn(a, b) => {
    a == b
};

// Diferente:
let neq_fn = fn(a, b) => {
    a != b
};

// Menor:
let lt_fn = fn(a, b) => {
    a < b
};

let gt_fn = fn(a, b) => {
    a > b
};

let lte_fn = fn(a, b) => {
    a <= b
};

let gte_fn = fn(a, b) => {
    a >= b
};

let and_fn = fn(a, b) => {
    a && b
};

let or_fn = fn(a, b) => {
    a || b
};

